{
    "name"             : "ducor/notify",
    "license"          : "MIT",
    "description"      : "toastr.js, pnotify.js flush notifications for Laravel",
    "homepage"         : "https://ducor.io/notify",
    "keywords"         : [
        "toastr",
        "notification",
        "laravel"
    ],
    "authors"          : [
        {
            "name"    : "Ali Asraf",
            "email"   : "ducorteam@gmail.com",
            "homepage": "https://github.com/Ducor/notify",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php"               : "^7.3",
        "illuminate/session": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev"      : {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit"    : "^8.3 || ^9.0"
    },
    "autoload"         : {
        "psr-4": {
            "Ducor\\Notify\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev"     : {
        "psr-4": {
            "Ducor\\Notify\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "Ducor\\Notify\\NotifyServiceProvider"
            ],
            "aliases"  : {
                "Notify": "Ducor\\Notify\\Facades\\Notify"
            }
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
